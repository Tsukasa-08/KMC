#設定ファイル

#Monte Carlo Step per Particle : 1度のKMC内で1粒子あたり何回イベントを起こすか
MCSP=10

#平均を取る粒子数
AVERAGE=216

#拡散種を配置する数、今回だったらプロトン
NDIFFS=216

#電場のありなし、(あり=1, なし=0)
EFIELDON=1

#電場の強さ、10^()を入力,単位は[eV/Å]
#EFIELD=-2

#ジャンプ頻度補正の目安、0.1が参考値、10の対数値で入力する
CORRECT=-1

#ジャンプリストの平均ジャンプ距離(Å),いずれはプログラムで抽出できたほうがいいかも
DISTANCEJUMP=1.5

#温度(K)
TEMP=600

#軸方向(+x=1, +y=2, +z=3, -x=-1, -y=-2, -z=-3)
AXIS=1

#blocking_listのありなし
BLOCKING=1
